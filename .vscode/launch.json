    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Mode",
            "request": "launch",
            "type": "dart",
            "program": "lib/app/main.dart"
        },
        {
            "name": "Verbose Debug Mode",
            "request": "launch",
            "type": "dart",
            "program": "lib/app/main.dart",
            "toolArgs": [
                "-v"
            ]
        },
        {
            "name": "Release Mode",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/app/main.dart"
        },
        {
            "name": "Verbose Release Mode",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/app/main.dart",
            "toolArgs": [
                "-v"
            ]
        },
    ]
}